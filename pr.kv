Manager:
    Main:
    Form:
    Result:

<Main>:
    name:"main"
    GridLayout:
        cols:1
        Label:
            text:'Predection Form For Heart Attack'
            color:0,0,0,1
            font_size:30
        BoxLayout:
            padding:5
            Image:
                size_hint_y:None
                source:"neclogo.jpg"
                width:1000
                height:200
        
        FloatLayout:
            cols:1    
            Button:
                background_color: 0, 0, 1, 1
                size_hint:None,None
                size:100,100
                pos:360,25
                font_size: 40
                on_release:
                    app.root.current="form"
                    root.manager.transition.direction="left"
                Image:
                    source:'form.png'
                    size:120,120
                    pos:350,25

            


<Form>:
    name:"form" 
    nn:name


    slide_age:slide_age
    slide_trp:slide_trp
    slide_cholestrol:slide_cholestrol
    slide_ecg:slide_ecg
    slide_thalaz:slide_thalaz
    slide_oldpeak:slide_oldpeak
    slide_cheast:slide_cheast
    
    BoxLayout:
        size_hint: None,None
        size:800,600
        # Label:
        #     text:"Form"
        #     color:0,0,0,1
        ScrollView:
            size_hint_y:.80
            pos_hint: {'x':0,'y':.10 }
            do_scroll_x:False
            do_scroll_y:True

            BoxLayout:
                size_hint:None,None
                size:700,700
                GridLayout:
                    cols:2
                    Button:       
                        background_color: (1,1,1,1)
                        size_hint: None,None
                        size:90,50
                        pos:260,55
                        font_size:30
                        on_release:
                            app.root.current="main"
                            root.manager.transition.direction="right"
                        Image:
                            source:'menu.png'
                            size:100,100
                            pos:0,580
                    Label:
                        text:'Form'
                        
                    Label:
                        text:"Name"
                        color:0,0,0,1
                    TextInput:
                        id:name
                        hint_text:"Enter your name"
                        input_type:'text'
                        multiline:False

                    Label:
                        text:'Age'
                        color:0,0,0,1
                    # secodm method for age slider
                    # slide_text:slider_label
                    BoxLayout:
                        orientation: 'vertical'
                        size:root.width,root.height
                        cols:2
                        Label:
                            id:slide_age
                            text:"Age"
                            color:0,0,0,1
                            font_size:15
                        Slider:
                            min:4
                            max:99
                            step:1
                            orientation: 'horizontal'
                            on_value:root.slide_it_age(*args)
                            

            # radio button
                    Label:
                        text:'Gender'
                        color:0,0,0,1
                    GridLayout:
                        cols:4
                        Label:
                            id:'gen'
                            text:"Male" 
                            color:0,0,0,1         
                        CheckBox:
                            group:'sex'
                            on_active:root.checkbox_click_sex(self,self.active,'0')
                            color:0,0,0,1
                        Label:
                            id:'gen'
                            text:"Female"  
                            color:0,0,0,1                                      
                        CheckBox:
                            group:'sex'
                            on_active:root.checkbox_click_sex(self,self.active,'1')
                            color:0,0,0,1
                    # cheast pain need to be four values

                    Label:
                        text:"Cheast pain"
                        color:0,0,0,1 
                    BoxLayout:
                        orientation: 'vertical'
                        size:root.width,root.height
                        cols:2
                        Label:
                            id:slide_cheast
                            text:"Cheast Pain"
                            color:0,0,0,1
                            font_size:15
                        Slider:
                            min:0
                            max:3
                            step:1
                            orientation: 'horizontal'
                            on_value:root.slide_it_cp(*args)


                    Label:
                        text:'Rbp'
                        color:0,0,0,1
                    BoxLayout:
                        orientation: 'vertical'
                        size:root.width,root.height
                        cols:2
                        Label:
                            id:slide_trp
                            text:"rbp"
                            color:0,0,0,1
                            font_size:15
                        Slider:
                            min:90
                            max:200
                            step:1
                            orientation: 'horizontal'
                            on_value:root.slide_it_trp(*args)
                        
                    Label:
                        text:'Cholestrol'
                        color:0,0,0,1
                    BoxLayout:
                        orientation: 'vertical'
                        size:root.width,root.height
                        cols:2
                        Label:
                            id:slide_cholestrol
                            text:"Cholestrol"
                            color:0,0,0,1
                            font_size:15
                        Slider:
                            min:100
                            max:200
                            step:1
                            orientation: 'horizontal'
                            on_value:root.slide_it_cholestrol(*args) 

                    Label:
                        text:'Fbs'
                        color:0,0,0,1
                    GridLayout:
                        cols:4
                        Label:
                            id:'fbs'
                            text:"0" 
                            color:0,0,0,1         
                        CheckBox:
                            group:'fbs'
                            on_active:root.checkbox_click_fbs(self,self.active,'0')
                            color:0,0,0,1
                        Label:
                            id:'fbs'
                            text:"1"  
                            color:0,0,0,1                                      
                        CheckBox:
                            group:'fbs'
                            on_active:root.checkbox_click_fbs(self,self.active,'1')
                            color:0,0,0,1
                    Label:
                        text:'ECG'
                        color:0,0,0,1
                    BoxLayout:
                        orientation: 'vertical'
                        size:root.width,root.height
                        cols:2
                        Label:
                            id:slide_ecg
                            text:"Ecg"
                            color:0,0,0,1
                            font_size:15
                        Slider:
                            min:0
                            max:3
                            step:1
                            orientation: 'horizontal'
                            on_value:root.slide_it_ecg(*args)
                        
                    Label:
                        text:'Thalaz'
                        color:0,0,0,1
                    BoxLayout:
                        orientation: 'vertical'
                        size:root.width,root.height
                        cols:2
                        Label:
                            id:slide_thalaz
                            text:"Thalaz"
                            color:0,0,0,1
                            font_size:15
                        Slider:
                            min:60
                            max:220
                            step:1
                            orientation: 'horizontal'
                            on_value:root.slide_it_thalaz(*args)
                        
                    Label:
                        text:'Exong'
                        color:0,0,0,1
                    GridLayout:
                        cols:4
                        Label:
                            id:'exong'
                            text:"Yes" 
                            color:0,0,0,1         
                        CheckBox:
                            group:'exong'
                            on_active:root.checkbox_click_exong(self,self.active,'yes')
                            color:0,0,0,1
                        Label:
                            id:'exong'
                            text:"No"  
                            color:0,0,0,1                                      
                        CheckBox:
                            group:'exong'
                            on_active:root.checkbox_click_exong(self,self.active,'no')
                            color:0,0,0,1

                    Label:
                        text:'Old Peak'
                        color:0,0,0,1
                    BoxLayout:
                        orientation: 'vertical'
                        size:root.width,root.height
                        cols:2
                        Label:
                            id:slide_oldpeak
                            text:"Oldpeak"
                            color:0,0,0,1
                            font_size:15
                        Slider:
                            min:0
                            max:10
                            step:0.1
                            orientation: 'horizontal'
                            on_value:root.slide_it_oldpeak(*args) 

                    Label:
                        text:'Slope'
                        color:0,0,0,1
                    GridLayout:
                        cols:4
                        Label:
                            id:'slope'
                            text:"0" 
                            color:0,0,0,1         
                        CheckBox:
                            group:'slope'
                            on_active:root.checkbox_click_slope(self,self.active,'0')
                            color:0,0,0,1
                        Label:
                            id:'slope'
                            text:"1"  
                            color:0,0,0,1                                      
                        CheckBox:
                            group:'slope'
                            on_active:root.checkbox_click_slope(self,self.active,'1')
                            color:0,0,0,1
                
            
            
                    GridLayout:
                        cols:1 
                        FloatLayout:   
                            Button:
                                background_color: 0.4, 0.5, 0.6, 1
                                size_hint: None,None
                                size:150,50
                                # pos:305,25
                                pos:350,10
                                font_size:30
                                on_press:root.calc()
                                # on_release:
                                #     app.root.current="result"
                                #     root.manager.transition.direction="left"
                                Image:
                                    source:'submit.jpg'
                                    size:150,150
                                    pos:350,-50




<Result>:
    name:"result"
    rer: rer
    ini:ini
    wel:wel
    GridLayout:
        cols:1
        Label:
            id:ini
            pos_hint:{'x':0.1,'top':0.9}
            size_hint:0.8,0.2
            text:"Hi "
            color:0,0,0,1
        Label:
            id:wel
            pos_hint:{'x':0.1,'top':0.9}
            size_hint:0.8,0.2
            text:"Your report"
            color:0,0,0,1
        Label:
            id:rer
            pos_hint:{'x':0.1,'top':0.9}
            size_hint:0.8,0.2
            text:"Result be: "
            color:0,0,0,1

        FloatLayout:
            cols:2
            Button:       
                background_color: (1,1,1,1)
                size_hint: None,None
                size:90,50
                pos:260,50
                font_size:30
                on_release:
                    app.root.current="main"
                    root.manager.transition.direction="right"
                Image:
                    source:'menu.png'
                    size:100,100
                    pos:250,25
                    
            Button:
                background_color: 0.1, 0.2, 0.6, 1
                size_hint: None,None
                size:90,90
                pos:510,25
                font_size:30
                on_release:
                    app.root.current="form"
                    root.manager.transition.direction="right"
                Image:
                    source:'form.png'
                    size:120,120
                    pos:500,25